{
  "name": "preact-hooks",
  "amdName": "preactHooks",
  "version": "0.1.0",
  "private": true,
  "description": "Hook addon for Preact",
  "main": "dist/hooks.js",
  "module": "dist/hooks.module.js",
  "umd:main": "dist/hooks.umd.js",
  "source": "src/index.js",
  "license": "MIT",
  "types": "src/index.d.ts",
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "mangle": {
    "regex": "^_"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "browser": "./dist/hooks.module.js",
      "umd": "./dist/hooks.umd.js",
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.js"
    }
  }
}