{
  "name": "pos-newclick",
  "version": "0.6.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build --base-href ./ && electron .",
    "electron:start": "electron .",
    "electron:dev": "electron .",
    "build:angular": "ng build --configuration production",
    "build:electron": "electron-builder --win --x64",
    "bump:version": "node bump-version.js",
    "electron:buildPublish": "npm run bump:version && npm run build:angular && npm run build:electron"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.6",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@bhplugin/ng-datatable": "^0.1.0",
    "@capacitor/android": "^6.2.1",
    "@capacitor/cli": "^5.7.8",
    "@capacitor/core": "^5.7.8",
    "@dustfoundation/ngx-sortablejs": "^15.0.0",
    "@fullcalendar/angular": "^6.1.8",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/splash-screen": "^5.36.0",
    "@ionic-native/status-bar": "^5.36.0",
    "@ionic/angular": "^6.7.5",
    "@ng-select/ng-select": "^10.0.4",
    "@ngrx/store": "^15.4.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/pdfmake": "^0.2.9",
    "angular-custom-modal": "^1.2.0",
    "apexcharts": "^3.37.3",
    "electron-store": "^10.0.1",
    "extract-zip": "^2.0.1",
    "file-saver": "^2.0.5",
    "file-upload-with-preview": "^6.0.2",
    "fullcalendar": "^6.1.14",
    "headlessui-angular": "^0.0.7",
    "install": "^0.13.0",
    "ng-apexcharts": "^1.7.4",
    "ngx-bootstrap": "^10.3.0",
    "ngx-color-picker": "^16.0.0",
    "ngx-scrollbar": "^11.0.0",
    "ngx-virtual-scroller": "^4.0.3",
    "node-machine-id": "^1.1.12",
    "pdfmake": "^0.2.10",
    "ps-list": "^8.1.1",
    "rxjs": "~7.8.0",
    "sortablejs": "^1.15.0",
    "sweetalert2": "^11.11.0",
    "tslib": "^2.3.0",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.15.10",
    "@types/sortablejs": "^1.15.1",
    "autoprefixer": "^10.4.14",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.9.4"
  },
  "main": "main.js",
  "build": {
    "appId": "com.NewClick",
    "productName": "NewClick POS",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "assets/**/*"
    ],
    "icon": "icon.ico",
    "win": {
      "icon": "icon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "shortcutName": "NewClick POS",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "mac": {
      "target": "dmg"
    }
  }
}
